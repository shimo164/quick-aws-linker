{
	"name": "QuickAWS Linker",
	"description": "This Chrome extension allows you to seamlessly navigate to the AWS Lambda console, CloudWatch logs, and X-Ray traces.",
	"version": "1.2",
	"manifest_version": 3,
	"permissions": [
		"activeTab",
		"scripting",
		"tabs",
		"contextMenus",
		"storage"
	],
	"options_page": "options.html",
	"action": {
		"default_title": "QuickAWS Linker"
	},
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}
